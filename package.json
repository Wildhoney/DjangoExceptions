{
  "name": "django-exceptions",
  "version": "0.1.0",
  "description": "Handle and parse Django REST Framework validation messages with aplomb. ",
  "main": "dist/exceptions.js",
  "scripts": {
    "test": "npm run build && clear && printf \"\n\" && figlet -f \"Kban\" \"Tests!\" && ava",
    "build": "webpack",
    "postinstall": "bower i",
    "clean": "rm -rf node_modules & rm -rf bower_components & wait"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/DjangoExceptions.git"
  },
  "keywords": [
    "django",
    "rest",
    "framework",
    "exceptions",
    "messages",
    "validations",
    "parse",
    "handle",
    "graceful"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/DjangoExceptions/issues"
  },
  "homepage": "https://github.com/Wildhoney/DjangoExceptions#readme",
  "devDependencies": {
    "ava": "~0.18.2",
    "babel-loader": "~7.0.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-es2016": "~6.24.1",
    "babel-preset-es2017": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "figlet-cli": "~0.1.0",
    "purs-loader": "~3.0.0",
    "webpack": "~2.6.1"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
